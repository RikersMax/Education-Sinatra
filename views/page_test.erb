	<thead>
	<tr>
	<th>First name </th>
	<th>Last name </th>
	<th>Phone </th>
	<th>Date </th>
	<th>Gender </th>
	<th>Barber </th>
	<th>Summary </th>
	</tr>
	</thead>
	<tbody>
	<%= @show_user%>
	</tbody>
</table>

</div>











